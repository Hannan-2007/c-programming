int n;
    int m;
    int sum = 0;
    scanf("%d", &n);
    scanf("%d", &m);
    if (n <= 0 || m <= 0 || n >= 30 || m >= 30 || n > m)
    {
        printf("Invalid Input\n");
    } else 
    {
      for (int i = n; i <= m; i++)
      {
            if (i % 2 == 0) 
            {
                sum += i;
            }
        }
        printf("%d\n", sum);
    }
    return 0;
}
