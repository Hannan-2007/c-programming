int num1, num2, num3, num4, num5;
    int sum;
    float average;
    scanf("%d",&num1);
    scanf("%d",&num2);
    scanf("%d",&num3);
    scanf("%d",&num4);
    scanf("%d",&num5);
    sum = num1 + num2 + num3 + num4 + num5;
    average = sum / 5.0;
    printf("Sum of the 5 number is : %d\n", sum);
    printf("Average of the 5 number is : %.1f\n", average);
    return 0;
}
