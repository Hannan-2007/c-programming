#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char x[100];
    int y;
    int z;
    scanf("%s", x);
    scanf("%d", &y);
    scanf("%d", &z);
    printf("Name of the Student:%s\n", x);
    if ((y == 1 && z > 70) || ((y == 1 || y == 2) && z > 75)) 
    {
        printf("%s is Eligible for Placement\n", x);
    } else 
    {
        printf("%s is Not Eligible for Placement\n", x);
    }
    return 0;
}
