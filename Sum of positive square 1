#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
  
    int n,a;
    int sum = 0;
    scanf("%d", &n);
    int arr[n];
    for(a= 0;a<n;a++) 
    {
    scanf("%d",&arr[a]);
        if(arr[a] > 0)
        {
         sum += arr[a]*arr[a];
        }
    }
    printf("%d\n",sum);
    return 0;
}
