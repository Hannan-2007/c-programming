#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int n;
    int num;
    int evenCount = 0; 
    int oddCount = 0;

    scanf("%d", &n);
    for (int i = 0; i < n; i++) 
    {
        scanf("%d", &num);
        if (num % 2 == 0)
        {
            evenCount++;
        } else 
        {
            oddCount++;
        }
    }
    if (evenCount == n) 
    {
        printf("Even\n");
    } else if (oddCount == n) 
    {
        printf("Odd\n");
    } else 
    {
        printf("Mixed\n");
    }
    return 0;
}
