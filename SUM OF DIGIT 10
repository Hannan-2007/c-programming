#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
        int number;
        int sum = 0;
    scanf("%d", &number);
    if (number >= 100) {
        int temp = number;
        while (temp > 0) {
            sum += temp % 10;
            temp /= 10;
        }
      printf("Sum of digit is %d\n", sum);
    } 
    else
    {
        printf("Invalid Input\n");
    }
    return 0;
}
